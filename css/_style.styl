.AAE-conten-page
    width 100%
    .AAE-header-nav
        width 100%
        border-bottom 1px solid rgba(255, 255, 255, 0.2);
        color grayNav 
        position fixed
        z-index 1000
        .navbar-brand 
            display: none
        .nav-item
            a  
                color grayBase
        .AAE-logo-menu
            img 
                margin: 12px
                width: 200px
                transition: 0.4s
            .AAE-header-logo-add 
                width: 140px
.AAE-header-nav-add
    background-color: grayDark
    border-bottom 1px solid rgba(20, 20, 20, 0.2)


.AAE-conten-one 
    background-image url(../images/fondo-inicio.jpg)
    background-position center center
    background-repeat no-repeat
    background-size cover
    overflow: hidden
    .AAE-conten-title 
        min-height: 100vh;
        // background-color: rgba(255, 255, 255, 0.2);
    .AAE-ancle-up
        height 40px
        font-size: 30px
        padding: 10px 10px 57px 10px
        position: fixed
        z-index 1000
        margin-top: 87vh
        background-color: rgba(20, 20, 20, 0.9)
        color: whiteGray
        border-radius: 200px
        display: none
    h2 
        font-family "Lato", sans-serif
        font-size 73px
        text-align center
        font-weight 100 
        font-style italic
        color whiteLight
        line-height 90%
        span  
            color yellowLight
            font-weight 300

.AAE-title 
    font-family: "Roboto", sans-serif 
    font-weight: 100 
    font-size: 23px
    text-align center   
    
.AAE-conten 
    width: 100%;
    padding-right: 15px;
    padding-left: 15px;
.AAE-conten-two
    width: 100% 
    background-color whiteGray 
    font-family: "Roboto", sans-serif
    font-weight: 300
    font-size: 19px
    h6 
        padding: 5px
    .AAE-sect-one
        min-height 7vh 
        background-color grayDark
        color whiteGray
        margin 0 -15px 0 -15px
    .AAE-sect-two
        background-color grayMaterialize
        margin 0 -15px 0 -15px
        color grayBase
        padding 30px 10% 30px 10%
        h6 
            color: yellowDark
            padding: 5px
    .AAE-sect-three
        background-color yellowDark
        margin 0 -15px 0 -15px
        color whiteGray
        border-bottom: 1vh solid bluegray
        padding 10px 13% 10px 13%
        color: grayMaterialize
    .AAE-sect-four 
        min-height 40vh
        margin 0 -12px 0 -12px
        color grayDark
        text-align center
        padding: 30px 0px 30px 0px
        .AAE-conten-iconSpam
            width: 150px
            height 150px
            padding: 25px
            border 3px solid grayDark
            border-radius: 200px
        img 
            width 150px 
        p  
            padding: 0px 50px 0px 50px
            font-size 16px 

