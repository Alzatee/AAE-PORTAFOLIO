    text-align center
    overflow: hidden;
    .carousel.slide
        margin 62px -15px 0px -15px
        .item-proyects 
            width: 400px
            height 200px
            img  
                width: 100%
    h6 
        margin-top 200px
    p 
        margin 10%
        span 
            color purpleDark
    .carousel-control-prev-icon
        width 50px
        height 50px
        background-color: purpleMaterialize
    .carousel-control-next-icon
        width 50px
        height 50px
        background-color: purpleMaterialize

.AAE-footer
    background-color: #1b1e24
    font-family "Roboto", sans-serif
