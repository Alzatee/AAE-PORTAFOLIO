        .AAE-items-services 
            background-color whiteLight
            border-radius 200px
            width: 115px
            height 115px
            padding: 20px
            .fab
                color: #f35a59
            .fas
                color: #0190D9
            .far
                color: yellowLight
                margin: 2px 0 0 -5px
        p 
            margin 10px
            span 
                color #0190D9
    .AAE-text-services 
        margin 3% 17% 3% 17%

.AAE-conten-five 
    background-color: blackDark
    font-family: "Roboto", sans-serif
    font-weight: 300
    font-size: 19px
