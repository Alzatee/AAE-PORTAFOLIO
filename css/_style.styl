.AAE-conten-four 
    width 100%
    background-repeat no-repeat
    background-size cover
    background-image url(../images/cohete.png)
    font-family: "Roboto", sans-serif
    font-weight: 300
    font-size: 19px
    color grayBase
    text-align center
    padding: 20px
    h6  
        margin 70px
    .AAE-content-services
        background-color grayMaterialize 
        border-radius 20px
        width: 250px 
        padding: 15px
        margin 10px
